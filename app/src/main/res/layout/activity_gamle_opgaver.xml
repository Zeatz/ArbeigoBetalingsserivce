<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nicolai.arbeigobetalingsserivce.GamleOpgaverActivity">

    <ImageView
        android:id="@+id/ArbeigoHeader"
        android:layout_width="wrap_content"
        android:layout_height="119dp"
        android:src="@drawable/headerarbeigo"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recylcerView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/ArbeigoHeader"></android.support.v7.widget.RecyclerView>



</RelativeLayout>
